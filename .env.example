# Krushak App - Environment Configuration
# Copy this file to .env and fill in your actual API keys

# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Google Gemini AI API
GEMINI_API_KEY=your-gemini-api-key-here

# Weather API (OpenWeatherMap)
OPENWEATHER_API_KEY=your-openweather-api-key-here

# Google Services
GOOGLE_API_KEY=your-google-api-key-here
GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# Firebase Cloud Messaging
FCM_SERVER_KEY=your-fcm-server-key-here
FIREBASE_PROJECT_ID=your-firebase-project-id

# Market Data APIs
AGMARKNET_API_KEY=your-agmarknet-api-key-here
COMMODITY_API_KEY=your-commodity-api-key-here

# Bank Integration APIs
SBI_API_KEY=your-sbi-api-key-here
HDFC_API_KEY=your-hdfc-api-key-here
ICICI_API_KEY=your-icici-api-key-here
PNB_API_KEY=your-pnb-api-key-here

# Location Services
LOCATION_IQ_API_KEY=your-locationiq-api-key-here

# Image Storage
CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret

# Analytics
MIXPANEL_TOKEN=your-mixpanel-token-here
GOOGLE_ANALYTICS_ID=your-ga-tracking-id

# Other Services
YOUTUBE_API_KEY=your-youtube-api-key-here
NEWS_API_KEY=your-news-api-key-here

# App Configuration
APP_VERSION=1.0.0
ENVIRONMENT=development
DEBUG_MODE=true

# Real-time Configuration
REALTIME_ENABLED=true
AI_AUTO_ANALYSIS_INTERVAL=120000
WEATHER_UPDATE_INTERVAL=600000
MARKET_UPDATE_INTERVAL=120000
NOTIFICATION_CHECK_INTERVAL=300000
